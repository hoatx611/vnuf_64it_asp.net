
@{
    ViewBag.Title = "Demo";
    Layout = "~/Views/Shared/_Base.cshtml";
}

@section inner{
    <!-- inner banner -->
    <section class="inner-banner py-5">
        <div class="w3l-breadcrumb py-lg-5">
            <div class="container pt-4 pb-sm-4">
                <h4 class="inner-text-title font-weight-bold pt-sm-5 pt-4">@ViewBag.Title</h4>
                <ul class="breadcrumbs-custom-path">
                    <li><a href="/TrangChu/">Trang chủ</a></li>
                    <li class="active"><i class="fas fa-angle-right mx-2"></i>@ViewBag.Title</li>
                </ul>
            </div>
        </div>
    </section>
    <!-- //inner banner -->
}

@section mains{
    <!-- pricing section -->
    <section class="w3l-pricing py-5">
        <div class="container py-lg-5 py-md-4 py-2">
            <h3 class="title-style text-center mb-sm-5 mb-4">CÁC ỨNG DỤNG <span>DEMO</span> !!!</h3>
            <div class="row t-in justify-content-center">
                <div class="col-lg-12 col-md-6 price-main-info">
                    <div class="price-inner card box-shadow">

                        <div class="card-body">
                            <ul class="list-unstyled mt-3 mb-4">
                                <li> <span class="fa fa-check"></span>
                                    <a href="/Demo/UploadFile">TEST: TẢI LÊN TẬP TIN</a>
                                </li>
                                <li>
                                    <span class="fa fa-check"></span>
                                    <a href="/Demo/UploadFile">TEST: GỬI MAIL</a>
                                </li>
                                <li class="disable"> <span class="fa fa-check"></span> Taper pants</li>
                                <li class="disable"> <span class="fa fa-check"></span> Tops short sleeve design</li>
                                <li class="disable"> <span class="fa fa-check"></span> Pants - shorten/lengthen</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- //pricing section -->
}

